#: //wa-plugins/shipping/flatrate/lib/config/plugin.php:6
msgid "Flat rate"
msgstr "Фиксированная стоимость"

#: //wa-plugins/shipping/flatrate/lib/config/plugin.php:7
msgid "Fixed shipping rate not depending on the shipment value and weight."
msgstr "Стоимость доставки, не зависящая от суммы и веса отправления."

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:8
msgid "Shipping rate"
msgstr "Стоимость доставки"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:9
msgid "Enter shipping rate as a flat rate, as a percent of cart total, or as a sum of both. Examples: <b>20</b>, <b>10%</b>, <b>20+10%</b>"
msgstr "Введите стоимость доставки в виде числа, процента от общей стоимости корзины покупок или в виде суммы этих слагаемых. Примеры: <b>500</b>, <b>10%</b>, <b>500+10%</b>"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:15
msgid "Currency"
msgstr "Валюта"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:16
msgid "Currency in which shipping rate is provided"
msgstr "Выберите валюту, в которой указана стоимость доставки"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:22
msgid "Estimated delivery time"
msgstr "Приблизительное время доставки"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:23
msgid "Average order transit time. Estimated delivery date will be calculated automatically and shown to customer."
msgstr "Средний срок доставки. Приблизительная дата доставки будет рассчитана автоматически и показана покупателю."

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:26
msgid "Same day"
msgstr "Этот же день"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:27
msgid "Next day"
msgstr "Следующий день"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:28
msgid "2 days"
msgstr "2 дня"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:29
msgid "3 days"
msgstr "3 дня"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:30
msgid "1 week"
msgstr "1 неделя"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:31
msgid "2 weeks"
msgstr "2 недели"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:32
msgid "Undefined"
msgstr "Не определено"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:37
msgid "Prompt for address"
msgstr "Запрашивать адрес"

#: //wa-plugins/shipping/flatrate/lib/config/settings.php:38
msgid "Request customer to fill in all address fields in case shipping address was not provided yet"
msgstr "Просить покупателя ввести адрес доставки, если он еще не был введен до расчета стоимости доставки"

#: //wa-plugins/shipping/flatrate/lib/flatrateShipping.class.php:36
msgid "Ground shipping"
msgstr "Наземная доставка"

#: //wa-plugins/shipping/flatrate/templates/form.html:8
msgid "Sample consignment note"
msgstr "Пример накладной"

#: //wa-plugins/shipping/flatrate/lib/flatrateShipping.class.php:97
msgid "Sample consignment description"
msgstr "Пример описания отправки"

#: //wa-plugins/shipping/flatrate/lib/flatrateShipping.class.php:119
msgid "Printable form not found"
msgstr "Печатная форма не найдена"

#: //wa-plugins/shipping/flatrate/templates/form.html:5
msgid "Sample shipping form"
msgstr "Пример формы доставки"

#: //wa-plugins/shipping/flatrate/templates/form.html:10
msgid "Destination address"
msgstr "Адрес доставки"

#: //wa-plugins/shipping/flatrate/templates/form.html:13
msgid "Order content"
msgstr "Содержимое заказа"

#: //wa-plugins/shipping/flatrate/templates/form.html:17
msgid "There are no items in the shipment"
msgstr "В заказе нет товаров"

#: //wa-plugins/shipping/flatrate/templates/form.html:20
msgid "Total amount"
msgstr "Итого"
