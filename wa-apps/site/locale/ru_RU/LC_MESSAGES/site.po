msgstr "Шаблоны уведомлений"

msgid "Priority over email notifications"
msgstr "Приоритет над email-уведомлениями"

msgid "Prefer SMS over email for sending notifications. In case of SMS sending failure, email will be sent as a replacement."
msgstr "Приоритет SMS над email при отправке уведомлений. В случае сбоя в отправке SMS будет отправлено email-уведомление."

msgid "A user must enter characters shown in an image"
msgstr "Пользователь должен ввести символы с изображения"

msgid "“Remember me” option"
msgstr "Флажок «Запомнить меня»"

msgid "Email or phone"
msgstr "Email или телефон"

msgid "Cannot disable both authorization options “Email” &amp; “Phone”"
msgstr "Нельзя отключать оба способа авторизации «Email» и «Телефон»"

msgid "Keep at least one of these two basic authorization option enabled for website visitors to be able to log into their accounts."
msgstr "Оставьте включенным хотя бы один из этих двух основных способов авторизации, чтобы посетители сайта могли входить в личный кабинет."

msgid "Authorization with an email address is used by default."
msgstr "По умолчанию используется авторизация с помощью email-адреса."

msgid "Services"
msgstr "Сервисы"

msgid "Invisible captcha will be used"
msgstr "Будет использована невидимая капча"

msgid "Authorization type"
msgstr "Тип авторизации"

msgid "Require captcha input"
msgstr "Требовать ли ввод капчи"

msgid "Login field placeholder"
msgstr "Плейсхолдер в поле логина"

msgid "Communication channel selection"
msgstr "Выбор канала связи"

msgid "Login, signup, and personal account"
msgstr "Вход, регистрация и личный кабинет"

msgid "Delete this site alias? It will <strong>not</strong> delete any data of the main site."
msgstr "Удалить этот сайт-зеркало? Никакие данные основного сайта <strong>не&nbsp;будут</strong> удалены."

msgid "Waiting period in seconds before sending next verification code:"
msgstr "Интервал в секундах перед отправкой следующего кода подтверждения:"

msgid "Verification code re-sending"
msgstr "Повторная отправка кода подтверждения"

msgid "Add SMS notification templates in <a href=\"%s\">Settings</a> app."
msgstr "Добавьте шаблоны SMS-уведомлений в приложении «<a href=\"%s\">Настройки</a>»."

msgid "Authorization by login name"
msgstr "Вход по имени пользователя"

msgid "Enable to allow backend users to log in on the website with their login names. Only login names different from phone numbers, i.e. containing characters other than digits, are supported."
msgstr "Включите, чтобы пользователи бекенда могли входить на сайт со своими логинами. Поддерживаются имена пользователей, отличающиеся от телефонных номеров, т. е. состоящие не только из цифр."

msgid "No sender email address specified in “<a href=\"%s\">Email settings</a>” section of Settings app."
msgstr "Не заполнен email-адрес отправителя в разделе «<a href=\"%s\">Настройки email</a>» приложения «Настройки»."

msgid "Language"
msgstr "Язык"

msgid "App [%s] deleted or disabled."
msgstr "Приложение [%s] удалено или отключено."

msgid "Email"
msgstr ""

msgid "Phone"
msgstr "Телефон"

msgid "Edit the list in “<a href=\"%s\">Email&nbsp;templates</a>” section."
msgstr "Измените список в разделе «<a href=\"%s\">Email-шаблоны</a>»."

msgid "Required field"
msgstr "Обязательное поле"

msgid "National direct-dialing code (NDD)"
msgstr "Код междугородней связи"

msgid "Enter the NDD code which is used for making calls within your country."
msgstr "Введите код, с помощью которого совершаются междугородние звонки в вашей стране. Пример кода для Российской Федерации: 8."

msgid "The NDD, if specified, will be replaced with the country code in website visitors’ phone numbers."
msgstr "Если указан код междугородней связи, то в номерах посетителей сайта он будет заменен на телефонный код страны."

msgid "Country code"
msgstr "Код страны"

msgid "Enter your country’s phone number code to be used in website visitors’ phone numbers instead of the NDD."
msgstr "Международный код вашей страны, который будет использоваться в телефонных номерах посетителей сайта вместо кода междугородней связи."

msgid "Enter digits only"
msgstr "Введите только цифры"

msgid "Temporary redirecting"
msgstr "Временное перенаправление"

msgid "Use server response code 302"
msgstr "Использовать код ответа сервера 302"

msgid "Enable for <strong>temproray</strong> redirecting only, which you are planning to cancel in the future."
msgstr "Включите только для <strong>временного</strong> перенаправления, которое вы планируете отменить в будущем."

msgid "Disable for <strong>permanent</strong> redirecting with server response code 301, which is used to inform search engines that the new URL should be indexed instead of the old one."
msgstr "Выключите для использования <strong>постоянного</strong> перенаправления с кодом ответа сервера 301 — он сообщает поисковым системам, что новый URL должен индексироваться вместо старого."

msgid "Move rule %s/* to the bottom of the rule list."
msgstr "Перенесите правило %s/* в конец списка правил."

msgid "Rule cannot be saved"
msgstr "Невозможно сохранить правило"

msgid "Rule address contains unsupported character <strong class=\"highlighted\">%s</strong>."
msgstr "В адресе правила содержится недопустимый символ <strong class=\"highlighted\">%s</strong>."

msgid "“Custom text” rule is incorrect."
msgid_plural "%d “Custom text” rules are incorrect."
msgstr[0] "Правило «Произвольный текст» настроено неправильно."
msgstr[1] "%d правила «Произвольный текст» настроены неправильно."
msgstr[2] "%d правил «Произвольный текст» настроены неправильно."

msgid "“Redirect” rule is incorrect."
msgid_plural "%d “Redirect” rules are incorrect."
msgstr[0] "Правило «Перенаправление» настроено неправильно."
msgstr[1] "%d правила «Перенаправление» настроены неправильно."
msgstr[2] "%d правил «Перенаправление» настроены неправильно."

msgid "Homepage"
msgstr "Главная страница"
